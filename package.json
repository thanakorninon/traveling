{
  "name": "travel-planner",
  "description": "แอปพลิเคชันวางแผนการเดินทางที่ใช้งานง่าย มาพร้อมกับฟีเจอร์ครบครันสำหรับการจัดทริปเที่ยว",
  "version": "1.0.0",
  "type": "module",
  "author": "Travel Planner Team",
  "license": "MIT",
  "keywords": [
    "travel",
    "planner",
    "thailand",
    "tourism",
    "itinerary",
    "budget",
    "packing"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "docs": "jsdoc -c jsdoc.config.js",
    "lint": "eslint js/ --ext .js",
    "lint:fix": "eslint js/ --ext .js --fix",
    "format": "prettier --write \"**/*.{js,css,html,md}\"",
    "clean": "rm -rf dist docs",
    "serve": "vite preview --port 4173"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jsdoc": "^4.0.2",
    "prettier": "^3.1.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "jspdf": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/travel-planner.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/travel-planner/issues"
  },
  "homepage": "https://github.com/yourusername/travel-planner#readme"
}
